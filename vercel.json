{
  "version": 2,
  "builds": [
    {
      "src": "/TextToSpeech",  // Assuming this is the directory containing your Django project
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/TextToSpeech/wsgi.py"
    }
  ]
}
